move(1) :- !.

move(N) :-
    M is N-1,
    move(M),
    Count is 2.

halt.
goal :- move(1000000), halt.

?- goal








move(1):- !.


g(1,X).
f(X).


move(N) :- g(sss(1),Z), M is N-1, move(M), f(M).

halt.
goal :- move(1000000), halt.

?- goal






branch(0) :- !.

f(A,B,C,D,E,F).
g(X) :- f(a,b,c,d,e,f).

branch(N) :- N1 is N - 1,g(N1), branch(N1), g(N1).


halt.
goal :- branch(1000000), halt.


?- goal
